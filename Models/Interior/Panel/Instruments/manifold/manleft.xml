<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>man.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>visses</object-name>
    <object-name>needle1</object-name>
    <object-name>needle2</object-name>
	
    <object-name>vitre</object-name>
  </animation>

  <!-- working for Rembrandt -->
  <effect>
    <inherits-from>Aircraft/B-17/Models/Effects/Glass/glass</inherits-from>
    <object-name>vitre</object-name>
  </effect>
  
  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needle1</object-name>
    <object-name>needle2</object-name>
    <emission>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0.9 </red>
      <green> 0.5 </green>
      <blue>  0.2 </blue>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>needle1</object-name>
    <property>engines/engine[0]/mp-osi</property>
    <interpolation>
      <entry><ind>  0 </ind><dep>   0 </dep></entry>
      <entry><ind> 10 </ind><dep>  19 </dep></entry>
      <entry><ind> 15 </ind><dep>  50 </dep></entry>
      <entry><ind> 20 </ind><dep>  82 </dep></entry>
      <entry><ind> 25 </ind><dep> 114 </dep></entry>
      <entry><ind> 30 </ind><dep> 146 </dep></entry>
      <entry><ind> 35 </ind><dep> 180 </dep></entry>
      <entry><ind> 40 </ind><dep> 213 </dep></entry>
      <entry><ind> 45 </ind><dep> 246 </dep></entry>
      <entry><ind> 50 </ind><dep> 279 </dep></entry>
      <entry><ind> 55 </ind><dep> 310 </dep></entry>
      <entry><ind> 60 </ind><dep> 341 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
    <animation>
    <type>rotate</type>
    <object-name>needle2</object-name>
    <property>engines/engine[1]/mp-osi</property>
    <interpolation>
      <entry><ind>  0 </ind><dep>   0 </dep></entry>
      <entry><ind> 10 </ind><dep>  19 </dep></entry>
      <entry><ind> 15 </ind><dep>  50 </dep></entry>
      <entry><ind> 20 </ind><dep>  82 </dep></entry>
      <entry><ind> 25 </ind><dep> 114 </dep></entry>
      <entry><ind> 30 </ind><dep> 146 </dep></entry>
      <entry><ind> 35 </ind><dep> 180 </dep></entry>
      <entry><ind> 40 </ind><dep> 213 </dep></entry>
      <entry><ind> 45 </ind><dep> 246 </dep></entry>
      <entry><ind> 50 </ind><dep> 279 </dep></entry>
      <entry><ind> 55 </ind><dep> 310 </dep></entry>
      <entry><ind> 60 </ind><dep> 341 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
</PropertyList>
