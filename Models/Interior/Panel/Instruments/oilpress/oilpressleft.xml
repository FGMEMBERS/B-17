<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>oilpress.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>visses</object-name>
    <object-name>OilPres.Face</object-name>
    <object-name>OilPres.Needle1</object-name>
    <object-name>OilPres.Needle2</object-name>

    <object-name>vitre</object-name>
  </animation>

  <!-- working for Rembrandt -->
  <effect>
    <inherits-from>Aircraft/B-17/Models/Effects/Glass/glass</inherits-from>
    <object-name>vitre</object-name>
  </effect>
  
  <animation>
    <type>material</type>
    <object-name>OilPres.Face</object-name>
    <object-name>OilPres.Needle1</object-name>
    <object-name>OilPres.Needle2</object-name>
    <emission>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0.9 </red>
      <green> 0.5 </green>
      <blue>  0.2 </blue>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>OilPres.Needle1</object-name>
    <property>/engines/engine[0]/est-oilpress</property>
    <interpolation>
      <entry><ind>  0 </ind><dep>   0 </dep></entry>
      <entry><ind> 35 </ind><dep> 315 </dep></entry>
    </interpolation>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>OilPres.Needle2</object-name>
    <property>/engines/engine[1]/est-oilpress</property>
    <interpolation>
      <entry><ind>  0 </ind><dep>   0 </dep></entry>
      <entry><ind> 35 </ind><dep> 315 </dep></entry>
    </interpolation>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

</PropertyList>
