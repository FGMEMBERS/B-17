<?xml version="1.0" encoding="ISO-8859-1"?>

<!-- Red Tracer -->
<PropertyList>
  
  <path>tracer.ac</path>
  
  <animation>
    <enable-hot type="bool">false</enable-hot>
  </animation>
  
  <model>
    <path>Aircraft/F7F-Tigercat/Models/Effects/guns/smoke.xml</path>
    <name>smoke</name>
    <offsets>
      <x-m> 10.00000 </x-m>
      <y-m>  0.00000 </y-m>
      <z-m> -1.43238 </z-m>
    </offsets>
  </model>
  
  <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>tracer</object-name>
    <object-name>smoke</object-name>
  </effect>
  
  <animation>
    <type>dist-scale</type>
    <object-name>tracer</object-name>
    <factor>0.001</factor>
    <offset>0</offset>
  </animation>
  
  <animation>
    <type>material</type>
    <object-name>tracer</object-name>
    <emission>
      <factor>1</factor>
      <offset>1</offset>
      <red>   0.75 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
    </emission>
  </animation>
  
</PropertyList>
